<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="@dimen/linearLayout_padding">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvName_find_store"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:maxLines="1"
            android:textStyle="bold"
            android:textSize="@dimen/textView_textSize_find_store"
            android:textColor="@color/cardview_dark_background"
            android:text="textviewviewviewviewviewviewviewviewviewview " />

        <TextView
            android:textSize="@dimen/textView_textSize_find_store"
            android:id="@+id/tvKhoanCach_find_store"
            android:layout_width="@dimen/textView_layout_width"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/textView_layout_margin_left"
            android:text="9999 km" />
    </LinearLayout>

    <TextView
        android:textSize="@dimen/textView_textSize_find_store"
        android:id="@+id/tvDiaChi_find_store"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textColor="@color/cardview_dark_background"
        android:text="TextView" />

    <TextView
        android:id="@+id/tvLoai_find_store"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="TextView" />
</LinearLayout>
