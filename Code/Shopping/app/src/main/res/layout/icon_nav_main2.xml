<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_icon_nav_mani2"
        android:layout_width="@dimen/width_height_imageView"
        android:layout_height="@dimen/width_height_imageView"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/tvName_icon_nav_mani2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:lines="1"
        android:textSize="@dimen/textSize_tvName"
        android:text="TextView" />
</LinearLayout>
