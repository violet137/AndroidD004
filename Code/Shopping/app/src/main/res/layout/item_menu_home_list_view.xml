<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ViewFlipper
        android:id="@+id/vfAdvertise_item_menu"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="@dimen/viewFlipper_height">
    </ViewFlipper>

    <include
        android:visibility="gone"
        android:id="@+id/item_menu_product"
        layout="@layout/item_main_menu_product"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </include>

    <include
        android:visibility="gone"
        android:id="@+id/item_menu_new_product"
        layout="@layout/item_main_menu_new_product"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </include>

    <include
        android:visibility="gone"
        android:id="@+id/item_menu_fashion"
        layout="@layout/item_main_menu_fashion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </include>

    <include
        android:visibility="gone"
        android:id="@+id/item_menu_magazine"
        layout="@layout/item_main_menu_magazine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </include>
</LinearLayout>
